%ul.sf-menu.sf-expand-left.sf-skin-main-nav
  %li
    %a{ :href=>"#" } Main Menu
    %ul
      - 10.times do |i|
        %li
          - href = i%2 != 0 ? "#item_#{i}" : nil
          %a{ :href=>href }
            #{Faker::Lorem.words(4).join(" ")}
          - if i%2 == 0
            %ul
              - 3.times do |j|
                %li
                  <a href="#item_#{i}_#{j}">#{Faker::Lorem.words(4).join(" ")}</a>
